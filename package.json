{
  "name": "ysb-academy-",
  "version": "2.0.0",
  "main": "server.cjs",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.cjs",
    "dev": "nodemon server.cjs",
    "clean": "rimraf dist",
    "build": "npm run clean && babel assets/js --out-dir dist/js && npm run copy && npm run minify:html && npm run minify:css && npm run minify:js",
    "copy": "copyfiles -u 1 'assets/**/*.html' 'assets/**/*.css' 'assets/**/*.{png,jpg,svg}' dist",
    "minify:html": "html-minifier --input-dir dist --output-dir dist --file-ext html --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --use-short-doctype --minify-css true",
    "minify:css": "find dist/css -name '*.css' -exec cleancss -o {} {} \\;",
    "minify:js": "terser -c -m -o dist/js/output.min.js -- dist/js/*.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "metaapi.cloud-sdk": "^13.2.9",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "stripe": "^14.7.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.23.6",
    "clean-css-cli": "^5.6.3",
    "copyfiles": "^2.4.1",
    "html-minifier": "^4.0.0",
    "rimraf": "^5.0.5",
    "terser": "^5.26.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "description": ""
}
